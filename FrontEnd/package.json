{
    "name": "front-end",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --hmr --host=127.0.0.1 --ssl --ssl-cert ssl\\localhost.crt --ssl-key ssl\\localhost.key",
        "build": "ng build",
        "watch": "ng build --watch --configuration development"
    },
    "prettier": {
        "printWidth": 100,
